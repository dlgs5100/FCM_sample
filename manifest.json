{
  "manifest_version": 2,
  "name": "imwatchingu",
  "description": "Raspberry pi motion",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  
  "permissions": [
	"gcm",
	"storage",
    "activeTab",
	"notifications",
    "https://ajax.googleapis.com/"
  ],
  
  
  "background": {
    "scripts": ["background.js"]
  }
}